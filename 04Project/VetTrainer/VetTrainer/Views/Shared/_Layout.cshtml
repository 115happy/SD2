
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>@ViewBag.Title - @VetTrainer.Views.Strings.Global.AppName</title>
    @Styles.Render("~/Dist/css/bootstrap.min.css")
    @Scripts.Render("~/Src/js/jquery-3.1.1.js")
    @*@Scripts.Render("~/Dist/js/")*@ 
</head>

<body>
    <header>
        <nav class="navbar" role="banner">
            <div class="container">
                <div class="navbar-header">
                    <h1>
                        @Html.ActionLink(VetTrainer.Views.Strings.Global.AppName,"Index","Home",new {@class="nav-head"})
                    </h1>
                </div>
                
                <div class="collapse navbar-collapse navbar-right">
                    <ul class="nav navbar-nav">
                        <li><a href="">关于我们</a></li>                        
                    </ul>
                </div>
                <div style="width: auto;">
                @if (Request.IsAuthenticated)
                {
                    <strong>@Html.Encode(User.Identity.Name)</strong>
                    @Html.ActionLink("登出", "Logout", "Login")
                }
            </div> 
            </div><!--/.container-->
        </nav><!--/nav-->
        
    </header><!--/header-->
    
        @RenderBody()
  


    <footer class="container-fluid">
        <div class="row text-center">
            <p class="">&copy; copyright 2017 @VetTrainer.Views.Strings.Global.AppName</p>
 
        </div>
    </footer>

</body>

</html>