@model VetTrainer.Models.Role
@{
    ViewBag.Title = "ClinicSelect";
}

<h2>ClinicSelect</h2>
@{
    for (int i = 0; i < Model.Clinics.Count; i++)
    {
        string name = Model.Clinics[i].Name;
        @*<button/>*@
        @Html.ActionLink(name, "Clinic", "RolePlay", 
                new { roleId = Model.Id,clinicId= Model.Clinics[i].Id }, 
                new { @class = "btn btn-primary" })
    }
}

